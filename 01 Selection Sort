#include <bits/stdc++.h>
using namespace std;

int main() {
    
    //Selection Sort
    
    vector<int> v={8,7,6,5,4,3,2,1};
    int n=v.size();
    
    for(int i=0;i<n-1;i++)
        for(int j=i+1;j<n;j++)
            if(v[j]<v[i]) swap(v[j],v[i]);
        
    for(auto a: v) cout<<a<<" ";
    
    //Find the min element and swap with the ith index, after each iteration the (i+1)th smallest no will be at its place
    
    //Time - O(n), Space - O(1)
    
    return 0;
}
